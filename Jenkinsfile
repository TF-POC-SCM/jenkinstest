
pipeline{
    agent any
    stages{
        stage('Pre Build'){
            steps{
                echo 'This is pre build stage'
            }
        }
        stage('Build Stage'){
            steps{
                ehco 'This is Build stage'
            }
        }
        stage('Deploy Stage'){
            steps{
                ehco 'This is Build Stage'
            }
        }    
    }

}
